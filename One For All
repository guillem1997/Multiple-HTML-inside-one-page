<!DOCTYPE html>
<html>
<head>
	<meta charset="US-ASCII">
	<Title>Global Page</Title>
	<script type="text/javascript">
		function mostrar(identificador){
			for (i=1; i<4 ; i++){
				var documento = document.getElementById(i);
				if (documento.id == identificador){
					documento.style.display = 'block';
				} else{
					documento.style.display = 'none';
				}
			}	
		}
	</script>
</head>
<body>
	 <div id="1" style="display:block">
        <a href="#" onclick="mostrar('1');">Show Page 1</a>
        <a href="#" onclick="mostrar('2');">Show Page 2</a>
        <a href="#" onclick="mostrar('3');">Show Page 3</a><br>
        Content of page 1
     </div>
    
     <div id="2" style="display:none">
		<a href="#" onclick="mostrar('1');">Show Page 1</a>
        <a href="#" onclick="mostrar('2');">Show Page 2</a>
        <a href="#" onclick="mostrar('3');">Show Page 3</a><br>
        Content of page 2
     </div>

     <div id="3" style="display:none">
		<a href="#" onclick="mostrar('1');">Show Page 1</a>
        <a href="#" onclick="mostrar('2');">Show Page 2</a>
        <a href="#" onclick="mostrar('3');">Show Page 3</a><br>
        Content of page 3
     </div>
</body>
</html>
